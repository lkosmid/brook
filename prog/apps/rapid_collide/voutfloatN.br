void combineStreamsfloat (float (**streams)<>,
                          unsigned int num,
                          unsigned int width, 
                          unsigned int length,
                             float (*output)<>) {
   unsigned int  lengthmodmodulus=num!=0?(length%num):0;
   float tmp<(num*width),length>;
   streamSwap(tmp,*output);
   switch (num) {
   case 0:
      return;
   case 1:
      streamSwap(*output,*streams[0]);
      break;
   case 2:
      floatstreamCombine2f(*streams[0],
